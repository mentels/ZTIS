set termoption enhanced
save_encoding = GPVAL_ENCODING
set encoding utf8

set term png truecolor size 1024,768

set output "dochody_wydatki.png"

set arrow from 250,250 to 650,650

set xlabel "dochody na 1 mieszkańca"
set ylabel "wydatki na 1 mieszkańca"

plot 'dochody_wydatki.dat' u 3:4:2 w labels font ",9" rotate point pt 7 offset char 1,0 notitle

